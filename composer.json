{
  "name": "mistynetwork/discordwebhookapi",
  "description": "Simple discord webhook virion for PocketMine-MP",
  "minimum-stability": "dev",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "reacherurmom",
      "email": "reacherurmom@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "reacherurmom\\discordwebhookapi\\": "src/reacherurmom/discordwebhookapi"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/NetherGamesMC/libasynCurl"
    }
  ],
  "require-dev": {
    "php": "^8.2",
    "pocketmine/pocketmine-mp": "^5.25.2"
  },
  "require": {
    "nethergamesmc/libasyncurl": "dev-pm5"
  },
  "extra": {
    "virion": {
      "spec": "3.0",
      "namespace-root": "reacherurmom\\discordwebhookapi"
    }
  }
}